<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <!--
  ==============================================================================
  Definition
  ==============================================================================
  -->
  <xs:annotation>
    <xs:appinfo>awe Screen Schema</xs:appinfo>
    <xs:documentation xml:lang="en">This Schema defines the way an awe Screen must be structured</xs:documentation>
  </xs:annotation>

  <!--
  ==============================================================================
  Includes
  ==============================================================================
  -->
  <xs:include schemaLocation="general.xsd" />

  <!--
  ==============================================================================
  Elements
  ==============================================================================
  -->

  <!-- Screen Element -->
  <xs:element name="screen" type="screenType">
    <xs:unique name="TagUnique">
      <xs:selector xpath=".//tag" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="AccordionUnique">
      <xs:selector xpath=".//accordion" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="AccordionItemUnique">
      <xs:selector xpath=".//accordion-item" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="MessageUnique">
      <xs:selector xpath=".//message" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="TagSourceUnique">
      <xs:selector xpath=".//tag" />
      <xs:field xpath="@source" />
    </xs:unique>
    <xs:unique name="VideoSourceUnique">
      <xs:selector xpath=".//video" />
      <xs:field xpath="@source" />
    </xs:unique>
    <xs:unique name="ColumnNameUnique">
      <xs:selector xpath=".//column" />
      <xs:field xpath="@name" />
    </xs:unique>
    <xs:unique name="CriteriaIdUnique">
      <xs:selector xpath=".//criteria" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="ButtonIdUnique">
      <xs:selector xpath=".//button" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="GridIdUnique">
      <xs:selector xpath=".//grid" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="PivotTableIdUnique">
      <xs:selector xpath=".//pivot-table" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="ContextButtonNameUnique">
      <xs:selector xpath=".//context-button" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="TabIdUnique">
      <xs:selector xpath=".//tab" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="WidgetIdUnique">
      <xs:selector xpath=".//widget" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="ChartIdUnique">
      <xs:selector xpath=".//chart" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="ContextSeparatorNameUnique">
      <xs:selector xpath=".//context-separator" />
      <xs:field xpath="@id" />
    </xs:unique>
    <xs:unique name="DialogIdUnique">
      <xs:selector xpath=".//dialog" />
      <xs:field xpath="@id" />
    </xs:unique>
  </xs:element>

  <!-- Grid Element -->
  <xs:element name="grid" type="gridType" />
  
  <!-- Accordion Element -->
  <xs:element name="accordion" type="accordionType" />
  
  <!-- Accordion Item -->
  <xs:element name="accordion-item" type="accordionItemType" />

  <!-- PivotTable Element -->
  <xs:element name="pivot-table" type="pivotTableType" />

  <!-- Tag Element -->
  <xs:element name="tag" type="elementType" />

  <!-- Video Element -->
  <xs:element name="video" type="videoType" />

  <!-- Javascript Element -->
  <xs:element name="javascript" type="javascriptType" />

  <!-- Dialog Element -->
  <xs:element name="dialog" type="dialogType" />

  <!-- Include Element -->
  <xs:element name="include" type="includeType" />

  <!-- Tab Element -->
  <xs:element name="tab" type="tabType" />

  <!-- Tab Element -->
  <xs:element name="tabcontainer" type="elementType" />

  <!-- Wizard Element -->
  <xs:element name="wizard" type="wizardType" />

  <!-- Wizard Panel Element -->
  <xs:element name="wizard-panel" type="elementType" />

  <!-- Column Element -->
  <xs:element name="column" type="columnType" />

  <!-- Column Element -->
  <xs:element name="group-header" type="groupHeaderType" />

  <!-- Window Element -->
  <xs:element name="window" type="windowType" />

  <!-- Resizable Element -->
  <xs:element name="resizable" type="resizableType" />

  <!-- Criteria Element -->
  <xs:element name="criteria" type="criteriaType" />

  <!-- Button Element -->
  <xs:element name="button" type="buttonType" />

  <!-- ContextButton Element -->
  <xs:element name="context-button" type="contextButtonType" />

  <!-- ContextSeparator Element -->
  <xs:element name="context-separator" type="contextSeparatorType" />

  <!-- Button Action Element -->
  <xs:element name="button-action" type="actionType" />

  <!-- Message Element -->
  <xs:element name="message" type="messageType" />

  <!-- Text Element -->
  <xs:element name="text" />

  <!-- Info Element -->
  <xs:element name="info" type="infoType" />

  <!-- Info Button Element -->
  <xs:element name="info-button" type="infoButtonType" />

  <!-- Info Criteria Element -->
  <xs:element name="info-criteria" type="infoCriteriaType" />

  <!-- Info Element -->
  <xs:element name="menu-container" type="menuContainerType" />

  <!-- Frame Element -->
  <xs:element name="frame" type="frameType" />

  <!-- View Element -->
  <xs:element name="view" type="viewType" />

  <!-- Dependency Element -->
  <xs:element name="dependency" type="dependencyType" />

  <!-- Dependency Action Element -->
  <xs:element name="dependency-action" type="actionType" />

  <!-- Dependency Element -->
  <xs:element name="dependency-element" type="dependencyElementType" />

  <!-- Widget Element -->
  <xs:element name="widget" type="widgetType" />

  <!-- Widget Parameter Element -->
  <xs:element name="widget-parameter" type="widgetParameterType" />

  <!-- Chart Element -->
  <xs:element name="chart" type="chartType" />

  <!-- Chart legend Element -->
  <xs:element name="chart-legend" type="chartLegendType" />

  <!-- Chart tooltip Element -->
  <xs:element name="chart-tooltip" type="chartTooltipType" />

  <!-- Chart xAxis Element -->
  <xs:element name="x-axis" type="chartAxisType" />

  <!-- Chart yAxis Element -->
  <xs:element name="y-axis" type="chartAxisType" />

  <!-- Chart Series Element -->
  <xs:element name="chart-serie" type="chartSerieType" />

  <!-- Chart Parameter Element -->
  <xs:element name="chart-parameter" type="chartParameterType" />

  <!-- TagList Element -->
  <xs:element name="tag-list" type="tagListType" />

  <!--
  ==============================================================================
  Attribute Groups
  ==============================================================================
  -->

  <!-- Server Call Attributes -->
  <xs:attributeGroup name="serverCallAttr">
    <xs:attribute name="initial-load" type="initialLoadEnum" />
    <xs:attribute name="server-action" type="serverActionEnum" />
    <xs:attribute name="target-action" type="nameEnum" />
    <xs:attribute name="max" type="xs:integer" />
    <xs:attribute name="autorefresh" type="xs:integer" />
    <xs:attribute name="autoload" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Screen Attributes -->
  <xs:attributeGroup name="screenAttr">
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="template" use="required" type="xs:string" />
    <xs:attribute name="target" type="xs:string" />
    <xs:attribute name="onload" type="xs:string" />
    <xs:attribute name="onunload" type="xs:string" />
    <xs:attribute name="keep-criteria" type="booleanEnum" />
    <xs:attribute name="help" type="xs:string" />
    <xs:attribute name="help-image" type="xs:string" />
  </xs:attributeGroup>

  <!-- Element Attributes -->
  <xs:attributeGroup name="elementAttr">
    <xs:attribute name="id" type="xs:string" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="source" type="xs:string" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="type" type="xs:string" />
    <xs:attribute name="expandible" type="expandibleTypes" />
  </xs:attributeGroup>
  
  <!-- Accordion Attributes -->
  <xs:attributeGroup name="accordionAttr">
    <xs:attribute name="id" type="xs:string" use="required" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="selected" type="xs:string" />
    <xs:attribute name="autocollapse" type="booleanEnum" />
  </xs:attributeGroup>  
  
  <!-- Accordion Item Attributes -->
  <xs:attributeGroup name="accordionItemAttr">
    <xs:attribute name="id" type="xs:string" use="required" />
    <xs:attribute name="label" type="xs:string" use="required" />
    <xs:attribute name="style" type="xs:string" />
  </xs:attributeGroup>

  <!-- Video Attributes -->
  <xs:attributeGroup name="videoAttr">
    <xs:attribute name="id" type="xs:string" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="source" type="xs:string" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="type" type="xs:string" />
    <xs:attribute name="expandible" type="expandibleTypes" />
    <xs:attribute name="loop" type="booleanEnum" />
    <xs:attribute name="preload" type="videoPreloadValues" />
    <xs:attribute name="autoplay" type="booleanEnum" />
    <xs:attribute name="poster" type="xs:string" />
    <xs:attribute name="src" type="xs:string" />
    <xs:attribute name="controls" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Widget Attributes -->
  <xs:attributeGroup name="widgetAttr">
    <xs:attributeGroup ref="serverCallAttr" />
    <xs:attribute name="type" type="xs:string" />
    <xs:attribute name="component" type="widgetComponentType" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="help" type="xs:string" />
    <xs:attribute name="help-image" type="xs:string" />
    <xs:attribute name="visible" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Widget Parameter Attributes -->
  <xs:attributeGroup name="widgetParameterAttr">
    <xs:attribute name="type" use="required" type="widgetParameterTypeEnum" />
    <xs:attribute name="value" use="required" type="xs:string" />
  </xs:attributeGroup>

  <!-- Chart Attributes -->
  <xs:attributeGroup name="chartAttr">
    <xs:attribute name="id" use="required" type="nameEnum" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="subtitle" type="xs:string" />
    <xs:attribute name="type" use="required" type="chartTypesEnum" />
    <xs:attribute name="stock-chart" type="booleanEnum" />
    <xs:attribute name="theme" type="xs:string" />
    <xs:attribute name="inverted" type="booleanEnum" />
    <xs:attribute name="stacking" type="stackTypeEnum" />
    <xs:attribute name="enable-data-labels" type="booleanEnum" />
    <xs:attribute name="format-data-labels" type="xs:string" />
    <xs:attribute name="zoom-type" type="axisEnum" />
    <xs:attribute name="help" type="xs:string" />
    <xs:attribute name="help-image" type="xs:string" />
    <xs:attribute name="icon-loading" type="loadingIconEnum" default="spinner" />
    <xs:attribute name="visible" type="booleanEnum" />
    <xs:attributeGroup ref="serverCallAttr" />
  </xs:attributeGroup>

  <!-- Chart Legend Attributes -->
  <xs:attributeGroup name="chartLegendAttr">
    <xs:attribute name="enabled" type="booleanEnum" />
    <xs:attribute name="layout" type="expandibleTypes" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="border-width" type="xs:string" />
    <xs:attribute name="align" type="alignEnum" />
    <xs:attribute name="verticalAlign" type="verticalAlignEnum" />
    <xs:attribute name="floating" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Chart Tooltip Attributes -->
  <xs:attributeGroup name="chartTooltipAttr">
    <xs:attribute name="enabled" type="booleanEnum" />
    <xs:attribute name="crosshairs" type="axisEnum" />
    <xs:attribute name="number-decimals" type="xs:string" />
    <xs:attribute name="suffix" type="xs:string" />
    <xs:attribute name="prefix" type="xs:string" />
    <xs:attribute name="point-format" type="xs:string" />
    <xs:attribute name="date-format" type="xs:string" />
    <xs:attribute name="shared" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Chart Axis Attributes -->
  <xs:attributeGroup name="chartAxisAttr">
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="label-format" type="xs:string" />
    <xs:attribute name="formatter-function" type="formatterFunctionEnum" />
    <xs:attribute name="label-rotation" type="xs:string" />
    <xs:attribute name="type" type="axisTypeEnum" />
    <xs:attribute name="tick-interval" type="xs:string" />
    <xs:attribute name="allow-decimal" type="booleanEnum" />
    <xs:attribute name="opposite" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Chart Series Attributes -->
  <xs:attributeGroup name="chartSeriesAttr">
    <xs:attribute name="id" type="xs:string" use="required" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="type" type="serieTypesEnum" />
    <xs:attribute name="color" type="xs:string" />
    <xs:attribute name="x-axis" type="xs:string" />
    <xs:attribute name="y-axis" type="xs:string" />
    <xs:attribute name="x-value" type="xs:string" use="required" />
    <xs:attribute name="y-value" type="xs:string" use="required" />
    <xs:attribute name="z-value" type="xs:string" />
    <xs:attribute name="drilldown-serie" type="xs:string" />
    <xs:attribute name="drilldown" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Chart Parameter Attributes -->
  <xs:attributeGroup name="chartParameterAttr">
    <xs:attribute name="type" use="required" type="chartParameterTypeEnum" />
    <xs:attribute name="name" use="required" type="nameEnum" />
    <xs:attribute name="value" type="xs:string" />
  </xs:attributeGroup>

  <!-- Tag Attributes -->
  <xs:attributeGroup name="tagAttr">
    <xs:attribute name="id" type="xs:string" />
    <xs:attribute name="source" type="xs:string" />
    <xs:attribute name="type" type="xs:string" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="template" type="xs:string" />
    <xs:attribute name="expandible" type="expandibleTypes" />
  </xs:attributeGroup>

  <!-- TagList Attributes -->
  <xs:attributeGroup name="tagListAttr">
    <xs:attributeGroup ref="serverCallAttr" />
    <xs:attribute name="type" type="xs:string" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="id" type="xs:string" />
  </xs:attributeGroup>

  <!-- Criteria Attributes -->
  <xs:attributeGroup name="criteriaAttr">
    <xs:attribute name="component" type="criteriaComponentType" use="required" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="icon" type="xs:string" />
    <xs:attribute name="unit" type="xs:string" />
    <xs:attribute name="size" type="xs:string" />
    <xs:attribute name="area-rows" type="xs:string" />
    <xs:attribute name="placeholder" type="xs:string" />
    <xs:attribute name="optional" type="booleanEnum" />
    <xs:attribute name="value" type="xs:string" use="optional" default="" />
    <xs:attribute name="readonly" type="booleanEnum" />
    <xs:attribute name="variable" type="xs:string" />
    <xs:attribute name="property" type="xs:string" />
    <xs:attribute name="session" type="xs:string" />
    <xs:attribute name="validation" type="xs:string" />
    <xs:attribute name="number-format" type="xs:string" />
    <xs:attribute name="show-slider" type="booleanEnum" />
    <xs:attribute name="capitalize" type="booleanEnum" />
    <xs:attribute name="strict" type="booleanEnum" />
    <xs:attribute name="message" type="xs:string" />
    <xs:attribute name="printable" type="printableEnum" />
    <xs:attribute name="check-empty" type="booleanEnum" />
    <xs:attribute name="check-initial" type="booleanEnum" />
    <xs:attribute name="check-target" type="xs:string" />
    <xs:attribute name="specific" type="xs:string" />
    <xs:attribute name="checked" type="booleanEnum" />
    <xs:attribute name="timeout" type="xs:integer" />
    <xs:attribute name="group" type="xs:string" />
    <xs:attribute name="destination" type="xs:string" />
    <xs:attribute name="help" type="xs:string" />
    <xs:attribute name="help-image" type="xs:string" />
    <xs:attribute name="show-weekends" type="xs:boolean" />
    <xs:attribute name="show-future-dates" type="xs:boolean" />
    <xs:attribute name="date-format" type="xs:string" />
    <xs:attribute name="date-show-today-button" type="booleanEnum" />
    <xs:attribute name="date-view-mode" type="viewModeEnum" />
    <xs:attribute name="left-label" type="xs:integer" />
    <xs:attribute name="visible" type="booleanEnum" />
    <xs:attributeGroup ref="serverCallAttr" />
  </xs:attributeGroup>

  <!-- Button Attributes -->
  <xs:attributeGroup name="buttonAttr">
    <xs:attribute name="id" type="nameEnum" use="required" />
    <xs:attribute name="type" type="xs:string" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="value" type="xs:string" />
    <xs:attribute name="size" type="xs:string" />
    <xs:attribute name="button-type" type="buttonTypes" />
    <xs:attribute name="icon" type="xs:string" />
    <xs:attribute name="help" type="xs:string" />
    <xs:attribute name="help-image" type="xs:string" />
  </xs:attributeGroup>

  <!-- Window Attributes -->
  <xs:attributeGroup name="windowAttr">
    <xs:attribute name="id" type="nameEnum" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="icon" type="xs:string" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="expandible" type="expandibleTypes" />
    <xs:attribute name="maximize" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Resizable Attributes -->
  <xs:attributeGroup name="resizableAttr">
    <xs:attribute name="id" type="nameEnum" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="icon" type="xs:string" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="expandible" type="expandibleTypes" />
    <xs:attribute name="directions" type="xs:string" />
  </xs:attributeGroup>

  <!-- Tab Attributes -->
  <xs:attributeGroup name="tabAttr">
    <xs:attribute name="id" use="required" type="nameEnum" />
    <xs:attribute name="initial-load" use="required" type="initialLoadEnum" />
    <xs:attribute name="target-action" use="required" type="nameEnum" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="maximize" type="booleanEnum" />
  </xs:attributeGroup>

  <!-- Wizard Attributes -->
  <xs:attributeGroup name="wizardAttr">
    <xs:attribute name="id" use="required" type="nameEnum" />
    <xs:attribute name="initial-load" use="required" type="initialLoadEnum" />
    <xs:attribute name="target-action" use="required" type="nameEnum" />
    <xs:attribute name="label" type="xs:string" />
  </xs:attributeGroup>

  <!-- Grid Attributes -->
  <xs:attributeGroup name="gridAttr">
    <xs:attribute name="id" use="required" type="nameEnum" />
    <xs:attribute name="name" type="nameEnum" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="multiselect" type="booleanEnum" />
    <xs:attribute name="checkbox-multiselect" type="booleanEnum" />
    <xs:attribute name="send-operations" type="booleanEnum" />
    <xs:attribute name="editable" type="booleanEnum" />
    <xs:attribute name="show-totals" type="booleanEnum" />
    <xs:attribute name="enable-filters" type="booleanEnum" />
    <xs:attribute name="row-numbers" type="booleanEnum" />
    <xs:attribute name="treegrid" type="booleanEnum" />
    <xs:attribute name="tree-id" type="xs:string" />
    <xs:attribute name="tree-parent" type="xs:string" />
    <xs:attribute name="tree-leaf" type="xs:string" />
    <xs:attribute name="expand-column" type="xs:string" />
    <xs:attribute name="icon-expand" type="xs:string" />
    <xs:attribute name="icon-collapse" type="xs:string" />
    <xs:attribute name="icon-loading" type="loadingIconEnum" default="spinner" />
    <xs:attribute name="initial-level" type="xs:integer" />
    <xs:attribute name="icon-leaf" type="xs:string" />
    <xs:attribute name="load-all" type="booleanEnum" />
    <xs:attribute name="send-all" type="booleanEnum" />
    <xs:attribute name="help" type="xs:string" />
    <xs:attribute name="help-image" type="xs:string" />
    <xs:attribute name="validate-on-save" type="booleanEnum" />
    <xs:attribute name="pagination-disabled" type="booleanEnum" />
    <xs:attribute name="pager-values" type="xs:string" />
    <xs:attribute name="visible" type="booleanEnum" />
    <xs:attribute name="row-height" type="xs:integer" />
    <xs:attributeGroup ref="serverCallAttr" />
  </xs:attributeGroup>

  <!-- PivotTable Attributes -->
  <xs:attributeGroup name="pivotTableAttr">
    <xs:attribute name="id" use="required" type="nameEnum" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="total-column-placement" type="PivotColumnPlacementEnum" />
    <xs:attribute name="total-row-placement" type="PivotRowPlacementEnum" />
    <xs:attribute name="cols" type="xs:string" />
    <xs:attribute name="rows" type="xs:string" />
    <xs:attribute name="renderer" type="PivotRendererEnum" />
    <xs:attribute name="aggregator" type="PivotAggregatorEnum" />
    <xs:attribute name="aggregation-field" type="xs:string" />
    <xs:attribute name="sort-method" type="PivotSortMethodEnum" />
    <xs:attribute name="decimal-numbers" type="xs:integer"/>
    <xs:attribute name="thousand-separator" type="xs:string"/>
    <xs:attribute name="decimal-separator" type="xs:string"/>    
    <xs:attributeGroup ref="serverCallAttr" />
  </xs:attributeGroup>

  <!-- Group Header Column Attributes -->
  <xs:attributeGroup name="columnGroupHeaderAttr">
    <xs:attribute use="required" name="name" type="nameEnum" />
    <xs:attribute name="label" type="xs:string" />
  </xs:attributeGroup>

  <!-- Column Attributes -->
  <xs:attributeGroup name="columnAttr">
    <xs:attribute name="name" use="required" type="nameEnum" />
    <xs:attribute name="style" type="xs:string" />
    <xs:attribute name="label" type="xs:string" />
    <xs:attribute name="value" type="xs:string" />
    <xs:attribute name="component" type="columnComponentType" />
    <xs:attribute name="icon" type="xs:string" />
    <xs:attribute name="unit" type="xs:string" />
    <xs:attribute name="type" type="cellDataTypes" />
    <xs:attribute name="server-action" type="serverActionEnum" />
    <xs:attribute name="target-action" type="nameEnum" />
    <xs:attribute name="initial-load" type="initialLoadEnum" />
    <xs:attribute name="autorefresh" type="xs:integer" />
    <xs:attribute name="align" type="alignEnum" />
    <xs:attribute name="charlength" type="xs:integer" />
    <xs:attribute name="width" type="xs:integer" />
    <xs:attribute name="sort-field" type="xs:string" />
    <xs:attribute name="hidden" type="booleanEnum" />
    <xs:attribute name="visible" type="booleanEnum" />
    <xs:attribute name="sortable" type="booleanEnum" />
    <xs:attribute name="movable" type="booleanEnum" />    
    <xs:attribute name="sendable" type="booleanEnum" />
    <xs:attribute name="optional" type="booleanEnum" />
    <xs:attribute name="summary-type" type="xs:string" />
    <xs:attribute name="number-format" type="xs:string" />
    <xs:attribute name="validation" type="xs:string" />
    <xs:attribute name="formatter" type="xs:string" />
    <xs:attribute name="format-options" type="xs:string" />
    <xs:attribute name="frozen" type="booleanEnum" />
    <xs:attribute name="pagebreak" type="booleanEnum" />
    <xs:attribute name="printable" type="printableEnum" />
    <xs:attribute name="help" type="xs:string" />
    <xs:attribute name="help-image" type="xs:string" />
    <xs:attribute name="check-initial" type="booleanEnum" />
    <xs:attribute name="checked" type="booleanEnum" />
    <xs:attribute name="readonly" type="booleanEnum" />
    <xs:attribute name="max" type="xs:string" />
    <xs:attribute name="dialog" type="xs:string" />
    <xs:attribute name="show-weekends" type="booleanEnum"/>
    <xs:attribute name="strict" type="xs:boolean"/>
  </xs:attributeGroup>

  <!-- Action Attributes -->
