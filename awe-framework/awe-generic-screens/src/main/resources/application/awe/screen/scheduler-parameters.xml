<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : scheduler-parameters.xml
    Created on : 10/11/2015
    Author     : David Fuentes
    Description: Ventana de parametros del planificador
-->
<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://aweframework.gitlab.io/awe/docs/schemas/screen.xsd" template="window" label="MENU_NEW_SCHEDULER_TASK" help="HELP_SCREEN_NEW_SCHEDULER_TASK">
  <tag source="Configuration">
    <criteria component="hidden" id="Ide" variable="IdeTsk.selected"/>
    <criteria label="PARAMETER_NAME" component="text" id="Nam" style="col-xs-12 col-sm-3" validation="required"/>
    <criteria label="PARAMETER_DESCRIPTION" component="textarea" id="Des" style="col-xs-9"/>
    <criteria label="PARAMETER_ACTIVE" component="select" id="Act" style="col-xs-4 col-sm-3" initial-load="enum" target-action="Es1Es0" validation="required" value="1"/>
    <criteria label="PARAMETER_STORE_EXECUTION" component="numeric" id="NumStoExe" style="col-xs-4 col-sm-3" property="sch.max.stored.executions"/>
    <criteria label="PARAMETER_TIMEOUT" component="numeric" id="TimOutExe" style="col-xs-4 col-sm-3"/>
    <criteria label="PARAMETER_EXECUTE" component="select" id="TypExe" initial-load="enum" target-action="ExeTyp" style="col-xs-6 col-sm-3" validation="required" />
    <criteria label="PARAMETER_EXECUTE_PATH" component="hidden" id="CmdExePth" style="col-xs-12 col-sm-3">
      <dependency initial="true" >
        <dependency-element id="TypExe" condition="eq" value="1"/>
        <dependency-action type="reset" target="CmdExePth"/>
      </dependency>
    </criteria>

    <tag id="executionCommand">
      <criteria id="CmdExe" label="PARAMETER_COMMAND" component="hidden">
        <dependency >
          <dependency-element id="TypExe"/>
          <dependency-action type="reset" target="ParameterList"/>
        </dependency>
        <dependency source-type="launcher" initial="true" target-type="input" target-action="maintainVal">
          <dependency-element id="TypExe" condition="eq" value="1"/>
          <dependency-element id="maintain" attribute="text" alias="maintainVal"/>
        </dependency>
        <dependency source-type="launcher" initial="true" target-type="input" target-action="commandVal">
          <dependency-element id="TypExe" condition="eq" value="0"/>
          <dependency-element id="command" attribute="text" alias="commandVal"/>
        </dependency>
        <dependency >
          <dependency-element id="TypExe" condition="eq" value="1"/>
          <dependency-element id="maintain"/>
          <dependency-action type="reset" target="ParameterList"/>
          <dependency-action type="server" server-action="data" target-action="loadMaintainVariables" target="ParameterList"/>
        </dependency>
      </criteria>
      <criteria label="PARAMETER_MAINTAIN" id="maintain" component="suggest" server-action="data" target-action="getPublicMaintainList"  style="col-xs-12 col-sm-3" >
        <dependency initial="true" target-type="set-required">
          <dependency-element id="TypExe" condition="eq" value="1"/>
        </dependency>
        <dependency initial="true" target-type="hide">
          <dependency-element id="TypExe" condition="eq" value="0"/>
        </dependency>
        <dependency initial="true">
          <dependency-element id="TypExe" condition="eq" value="0"/>
          <dependency-action type="reset" target="maintain"/>
        </dependency>
      </criteria>
      <criteria label="PARAMETER_COMMAND" id="command" component="text" style="col-xs-12 col-sm-3">
        <dependency initial="true" target-type="set-required">
          <dependency-element id="TypExe" condition="eq" value="0"/>
        </dependency>
        <dependency initial="true" target-type="hide">
          <dependency-element id="TypExe" condition="eq" value="1"/>
        </dependency>
        <dependency initial="true">
          <dependency-element id="TypExe" condition="eq" value="1"/>
          <dependency-action type="reset" target="command"/>
        </dependency>
      </criteria>
    </tag>
    <criteria label="PARAMETER_LAUNCH_DEPENDENCIES_WARN" component="checkbox" id="LchDepWrn" style="col-xs-12 no-label"/>
    <criteria label="PARAMETER_LAUNCH_DEPENDENCIES_ERROR" component="checkbox" id="LchDepErr" style="col-xs-12 no-label"/>
    <criteria label="PARAMETER_SET_WARNING_ERROR" component="checkbox" id="LchSetWrn" style="col-xs-12 no-label"/>
  </tag>
  <tag source="Parameters">
    <grid id="ParameterList" style="expand" send-operations="true" editable="true">
      <column name="IdePar" hidden="true"/>
      <column label="PARAMETER_NAME" name="ParNam" align="left" component="text" validation="required" charlength="30" sortable="false" />
      <column label="PARAMETER_SOURCE" name="ParSrc" align="left" component="select" validation="required" initial-load="enum" target-action="SchParSrc" charlength="20" sortable="false"/>
      <column label="PARAMETER_TYPE" name="ParTyp" align="left" component="select" validation="required" initial-load="enum" target-action="SchParTyp" charlength="20" sortable="false"/>
      <column label="PARAMETER_VALUE" name="ParVal" align="left" component="text" charlength="30" sortable="false"/>
      <button id="ButAddParameter" label="BUTTON_NEW" icon="plus">
        <button-action type="add-row" target="ParameterList" />
      </button>
      <button id="ButDelParameter" label="BUTTON_DELETE" icon="trash">
        <button-action type="delete-row" target="ParameterList" />
      </button>
      <dependency initial="true">
        <dependency-element id="Ide"/>
        <dependency-action type="server" server-action="data" target-action="AweSchTskParLst" target="ParameterList" />
      </dependency>
    </grid>
  </tag>
  <tag source="Launch">
    <criteria label="PARAMETER_LAUNCH_TYPE" component="select" id="TypLch" style="col-xs-12 col-sm-2 col-sm-offset-right-10" initial-load="enum" target-action="LchTyp" validation="required">
      <dependency initial="true">
        <dependency-element id="TypLch" condition="eq" value="0"/>
        <dependency-action type="add-class" async="true" target-action="hidden" target="#scheduled-launcher"/>
        <dependency-action type="add-class" async="true" target-action="hidden" target="#file-launcher"/>
      </dependency>
      <dependency initial="true">
        <dependency-element id="TypLch" condition="eq" value="1"/>
        <dependency-action type="remove-class" async="true" target-action="hidden" target="#scheduled-launcher"/>
        <dependency-action type="add-class" async="true" target-action="hidden" target="#file-launcher"/>
      </dependency>
      <dependency initial="true">
        <dependency-element id="TypLch" condition="eq" value="2"/>
        <dependency-action type="remove-class" async="true" target-action="hidden" target="#scheduled-launcher"/>
        <dependency-action type="remove-class" async="true" target-action="hidden" target="#file-launcher"/>
      </dependency>
    </criteria>
    <tag id="scheduled-launcher" type="div" style="row hidden">
      <tag id="repeat" type="div" style="col-xs-12 col-sm-12 col-lg-2">
        <criteria label="PARAMETER_REPEAT_EACH" component="numeric" id="RptNum" style="col-xs-7 col-sm-4 no-padding">
          <dependency initial="true" source-type="label" target-type="label" label="PARAMETER_CHECK_EACH">
            <dependency-element id="TypLch" condition="eq" value="2"/>
          </dependency>
          <dependency initial="true" source-type="label" target-type="label" label="PARAMETER_REPEAT_EACH">
            <dependency-element id="TypLch" condition="eq" value="1"/>
          </dependency>
          <dependency type="and" initial="true" target-type="set-required">
            <dependency-element id="TypLch" condition="ne" value="0"/>
            <dependency-element id="RptTyp" condition="ne" value="6"/>
            <dependency-element id="RptTyp" condition="ne" value="7"/>
          </dependency>
          <dependency type="or" target-type="set-readonly" initial="true">
            <dependency-element id="TypLch" condition="eq" value="0"/>
            <dependency-element id="RptTyp" condition="eq" value="6"/>
            <dependency-element id="RptTyp" condition="eq" value="7"/>
          </dependency>
          <dependency type="or" source-type="reset" target-type="input" initial="true">
            <dependency-element id="RptTyp" condition="eq" value="6"/>
            <dependency-element id="RptTyp" condition="eq" value="7"/>
          </dependency>
        </criteria>
        <criteria component="select" id="RptTyp" initial-load="enum" target-action="RptTyp" style="col-xs-7 col-sm-6 no-padding">
          <dependency initial="true" target-type="set-required">
            <dependency-element id="TypLch" condition="ne" value="0"/>
          </dependency>
          <dependency >
            <dependency-element id="RptTyp" />
            <dependency-action type="reset" target="scheduleParameters"/>
          </dependency>

          <!-- seconds dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="0"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>

          <!-- minutes dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="1"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>
          <!-- hours dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="2"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>
          <!-- days dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="3"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>
          <!-- months dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="4"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>
          <!-- years dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="5"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>

          <!-- once dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="6"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="add-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>

          <!-- custom dependencies -->
          <dependency initial="true">
            <dependency-element id="RptTyp" condition="eq" value="7"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#yearSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#monthSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daySelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#hourSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#execDateSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#execTimeSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#daysOfWeek"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#minuteSelector"/>
            <dependency-action type="remove-class" async="true" target-action="hidden" target="#secondSelector"/>
          </dependency>
        </criteria>
      </tag>

      <tag type="div" id="scheduleParameters">
        <criteria label="PARAMETER_CALENDAR" component="suggest" server-action="data" target-action="SchCalSug" id="IdeCal" style="col-xs-12 col-md-2" optional="true">
          <dependency target-type="set-readonly" initial="true">
            <dependency-element id="schExeDate"/>
          </dependency>
          <dependency target-type="hide" initial="true">
            <dependency-element id="RptTyp" condition="eq" value="6"/>
          </dependency>
        </criteria>
        <criteria label="PARAMETER_FROM" component="date" id="IniDat" style="col-xs-6 col-sm-3 col-md-2">
          <dependency target-type="set-readonly" initial="true">
            <dependency-element id="schExeDate"/>
          </dependency>
          <dependency target-type="hide" initial="true">
            <dependency-element id="RptTyp" condition="eq" value="6"/>
          </dependency>
          <dependency target-type="set-required" initial="true">
            <dependency-element id="IniTim"/>
          </dependency>
        </criteria>
        <criteria component="time" id="IniTim" show-future-dates="true" style="col-xs-6 col-sm-3 col-md-2">
          <dependency target-type="set-readonly" initial="true">
            <dependency-element id="schExeDate"/>
          </dependency>
          <dependency target-type="hide" initial="true">
            <dependency-element id="RptTyp" condition="eq" value="6"/>
          </dependency>
        </criteria>
        <criteria label="PARAMETER_TO" component="date" id="EndDat" show-future-dates="true" validation="{ge:{criterion:'IniDat',type:'date'}}" style="col-xs-6 col-sm-3 col-md-2">
          <dependency target-type="set-readonly" initial="true">
            <dependency-element id="schExeDate"/>
          </dependency>
          <dependency target-type="hide" initial="true">
            <dependency-element id="RptTyp" condition="eq" value="6"/>
          </dependency>
          <dependency target-type="set-required" initial="true">
            <dependency-element id="EndTim"/>
          </dependency>
        </criteria>
        <criteria component="time" id="EndTim" style="col-xs-6 col-sm-3 col-md-2">
          <dependency target-type="set-readonly" initial="true">
            <dependency-element id="schExeDate"/>
          </dependency>
          <dependency target-type="hide" initial="true">
            <dependency-element id="RptTyp" condition="eq" value="6"/>
          </dependency>
        </criteria>
        <tag type="div" style="col-xs-offset-0 col-lg-offset-2">
          <!--new criteria parameters-->
          <tag type="div" style="hidden" id="yearSelector">
            <criteria label="PARAMETER_YEARS" component="select-multiple" initial-load="query" target-action="yearSelectService" id="schExeYrs" style="col-xs-12 col-md-2" optional="true">
              <dependency target-type="set-readonly" initial="true">
                <dependency-element id="schExeDate"/>
              </dependency>
              <dependency source-type="formule" target-type="input" formule="*" initial="true">
                <dependency-element id="schExeYrs" condition="in" value="*"/>
                <dependency-action type="reset" target="schExeYrs"/>
              </dependency>
            </criteria>
            <criteria component="hidden" id="years">
              <dependency source-type="formule" initial="true" target-type="input" formule="'[schYearSelect]'">
                <dependency-element id="schExeYrs" attribute="value" alias="schYearSelect"/>
              </dependency>
              <dependency source-type="reset" target-type="input" initial="true">
                <dependency-element id="schExeYrs" condition="is empty"/>
              </dependency>
            </criteria>
          </tag>
          <tag type="div" style="hidden" id="monthSelector">
            <criteria label="PARAMETER_MONTHS" component="select-multiple" initial-load="enum" target-action="monthSelect" id="schExeMth" style="col-xs-12 col-md-2" optional="true">
              <dependency target-type="set-readonly" initial="true">
                <dependency-element id="schExeDate"/>
              </dependency>
            </criteria>
            <criteria component="hidden" id="months">
              <dependency source-type="formule" initial="true" target-type="input" formule="'[schMnthSelect]'">
                <dependency-element id="schExeMth" attribute="value" alias="schMnthSelect"/>
              </dependency>
              <dependency source-type="reset" target-type="input" initial="true">
                <dependency-element id="schExeMth" condition="is empty"/>
              </dependency>
            </criteria>
          </tag>
          <tag type="div" style="hidden" id="daySelector">
            <criteria label="PARAMETER_DAYS" component="select-multiple" initial-load="enum" target-action="daySelect" id="schExeDay" style="col-xs-12 col-md-2" optional="true">
              <dependency type="or" target-type="set-readonly" initial="true">
                <dependency-element id="weekDays"/>
                <dependency-element id="schExeDate"/>
              </dependency>
            </criteria>
            <criteria component="hidden" id="days">
              <dependency source-type="formule" initial="true" target-type="input" formule="'[schDaysSelect]'">
                <dependency-element id="schExeDay" attribute="value" alias="schDaysSelect"/>
              </dependency>
              <dependency source-type="reset" target-type="input" initial="true">
                <dependency-element id="schExeDay" condition="is empty"/>
              </dependency>
            </criteria>
          </tag>
          <tag type="div" style="hidden" id="daysOfWeek">
            <criteria label="PARAMETER_DAYS_OF_WEEK" component="select-multiple" initial-load="enum" target-action="weekSelect" id="schExeWeekDays" style="col-xs-12 col-md-2" optional="true">
              <dependency type="or" target-type="set-readonly" initial="true">
                <dependency-element id="days"/>
                <dependency-element id="schExeDate"/>
              </dependency>
            </criteria>
            <criteria component="hidden" id="weekDays">
              <dependency source-type="formule" initial="true" target-type="input" formule="'[schWeekSelect]'">
                <dependency-element id="schExeWeekDays" attribute="value" alias="schWeekSelect"/>
              </dependency>
              <dependency source-type="reset" target-type="input" initial="true">
                <dependency-element id="schExeWeekDays" condition="is empty"/>
              </dependency>
            </criteria>
          </tag>
          <tag type="div" style="hidden" id="hourSelector">
            <criteria label="PARAMETER_HOURS" component="select-multiple" initial-load="enum" target-action="hourSelect" id="schExeHour" style="col-xs-12 col-md-2" optional="true">
              <dependency target-type="set-readonly" initial="true">
                <dependency-element id="schExeTime"/>
              </dependency>
            </criteria>
            <criteria component="hidden" id="hours">
              <dependency source-type="formule" initial="true" target-type="input" formule="'[schHoursSelect]'">
                <dependency-element id="schExeHour" attribute="value" alias="schHoursSelect"/>
              </dependency>
              <dependency source-type="reset" target-type="input" initial="true">
                <dependency-element id="schExeHour" condition="is empty"/>
              </dependency>
            </criteria>
          </tag>
          <tag type="div" style="hidden" id="minuteSelector">
            <criteria label="PARAMETER_MINUTES" component="select-multiple" initial-load="enum" target-action="timeSelect" id="schExeMin" style="col-xs-12 col-md-2" optional="true">
              <dependency target-type="set-readonly" initial="true">
                <dependency-element id="schExeTime"/>
              </dependency>
            </criteria>
            <criteria component="hidden" id="minutes">
              <dependency source-type="formule" initial="true" target-type="input" formule="'[schMinSelect]'">
                <dependency-element id="schExeMin" attribute="value" alias="schMinSelect"/>
              </dependency>
              <dependency source-type="reset" target-type="input" initial="true">
                <dependency-element id="schExeMin" condition="is empty"/>
              </dependency>
            </criteria>
          </tag>
          <tag type="div" style="hidden" id="secondSelector">
            <criteria label="PARAMETER_SECONDS" component="select-multiple" initial-load="enum" target-action="timeSelect" id="schExeSec" style="col-xs-12 col-md-2" optional="true">
              <dependency target-type="set-readonly" initial="true">
                <dependency-element id="schExeTime"/>
              </dependency>
            </criteria>
            <criteria component="hidden" id="seconds">
              <dependency source-type="formule" initial="true" target-type="input" formule="'[schSecSelect]'">
                <dependency-element id="schExeSec" attribute="value" alias="schSecSelect"/>
              </dependency>
              <dependency source-type="reset" target-type="input" initial="true">
                <dependency-element id="schExeSec" condition="is empty"/>
              </dependency>
            </criteria>
          </tag>
          <tag type="div" style="hidden" id="execDateSelector">
            <criteria label="EXECUTION_DATE" component="date" show-future-dates="true" id="schExeDate" style="col-xs-10 col-md-2" optional="true">
              <dependency type="or" target-type="set-readonly" initial="true">
                <dependency-element id="years" condition="is not empty"/>
                <dependency-element id="months" condition="is not empty"/>
                <dependency-element id="days" condition="is not empty"/>
                <dependency-element id="weekDays" condition="is not empty"/>
                <dependency-element id="IdeCal" condition="is not empty"/>
                <dependency-element id="IniDat" condition="is not empty"/>
                <dependency-element id="IniTim" condition="is not empty"/>
                <dependency-element id="EndDat" condition="is not empty"/>
                <dependency-element id="EndTim" condition="is not empty"/>
              </dependency>
