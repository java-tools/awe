      <response>
        <type>message</type>
        <parameters>
          <parameter name="type" variable="MESSAGE_TYPE" />
          <parameter name="title" variable="MESSAGE_TITLE" />
          <parameter name="message" variable="MESSAGE_DESCRIPTION" />
