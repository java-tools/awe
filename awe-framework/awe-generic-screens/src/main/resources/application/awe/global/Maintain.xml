      <variable id="IdeThm" type="INTEGER" name="IdeThm" />
      <variable id="ScrIni" type="STRINGN" name="ScrIni" />
      <variable id="Res" type="STRINGN" name="Res" />
      <variable id="Act" type="INTEGER" name="Act" />
    </delete>
