{
  "presets": [
    ["env", {
      "targets": {
        "browsers": [
          "> 1%",
          "last 3 versions",
          "ie 8"
        ]
      },
      "debug": false,
      "useBuiltIns": true
    }]
  ],
  "env": {
    "development": {
      "compact": false,
      "plugins": [
        "transform-object-rest-spread",
        "transform-object-assign",
        "array-includes",
        "object-values-to-object-keys",
        "transform-object-entries",
        ["istanbul", {
          "exclude": [
            "**/test/",
            "**/node_modules/",
            "**/lib/"
          ]
        }]
      ]
    },
    "production": {
      "plugins": [
        "transform-object-rest-spread",
        "transform-object-assign",
        "array-includes",
        "object-values-to-object-keys",
        "transform-object-entries",
        ["istanbul", {
          "exclude": [
            "**/test/",
            "**/node_modules/",
            "**/lib/"
          ]
        }]
      ]
    }
  }
}
