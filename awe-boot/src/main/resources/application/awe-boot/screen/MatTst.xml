      <tabcontainer id="TreLoaMat">
        <window style="expand" expandible="vertical">
          <grid id="TreGrdLoa" treegrid="true" expand-column="TreGrdLoa_Nam" tree-id="TreGrdLoa_id" icon-expand="plus" icon-collapse="minus" icon-leaf="dot-circle-o" load-all="false" tree-parent="TreGrdLoa_parent"
