<?xml version="1.0" encoding="UTF-8"?>

<!-- Document : MatTst.xml Created on : 20 de Marzo de 2012 Author : Pablo GARCIA Description: Matrix Test Screen -->
<screen xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://aweframework.gitlab.io/awe/docs/schemas/screen.xsd" template="window" label="SCREEN_TITLE_MATRIX_TEST" help="HELP_SCREEN_TITLE_MATRIX_TEST">
  <tag source="buttons">
    <button label="BUTTON_SCREEN_HELP" icon="question-circle" id="ButHlp" help="HELP_HELP_BUTTON">
      <button-action type="dialog" target="HlpOpt" />
    </button>
    <button label="BUTTON_PRINT" icon="print" id="ButPrn" help="HELP_PRINT_BUTTON">
      <button-action type="validate" />
      <button-action type="dialog" target="PrnOpt" />
    </button>
    <button button-type="button" label="BUTTON_RELOAD" icon="refresh" id="ButRef">
      <!--<button-action type="server" server-action="data" target-action="QryEdiTst" target="GrdMus" /> -->
      <button-action type="filter" target="GrdMus" />
      <button-action type="select-first-row" target="GrdMus" />
      <button-action type="filter" target="TreGrd" />
      <button-action type="filter" target="TreGrdEdi" />
    </button>
    <button button-type="button" label="BUTTON_CONFIRM" icon="save" id="ButCnf" help="HELP_CONFIRM_BUTTON">
      <button-action type="validate" />
      <button-action type="screen" target="matrix_test" />
    </button>
    <button button-type="button" label="BUTTON_ADD_COLUMNS" icon="columns" id="ButAddCol">
      <button-action type="server" server-action="maintain-silent" target-action="AddColumns" />
    </button>
    <button button-type="button" label="BUTTON_REPLACE_COLUMNS" icon="retweet" id="ButRepCol">
      <button-action type="server" server-action="maintain-silent" target-action="ReplaceColumns" />
    </button>
  </tag>
  <tag source="center">
    <tab id="TabSelMat" initial-load="enum" target-action="EnuAweSelMat">
      <tabcontainer id="StaMat">
        <window style="expand" label="ENUM_MATRIX_STATIC" maximize="true" expandible="vertical">
          <grid id="GrdSta" style="expand" initial-load="query" server-action="data" target-action="QryUniTstId" max="30">
            <column label="PARAMETER_TEXT" name="Als" sort-field="Als" align="left" charlength="20" style="separator" />
            <group-header name="GrpHeaCol" label="PARAMETER_TEXT">
              <column label="PARAMETER_TEXT" name="Des" sort-field="Des" align="left" charlength="40" />
              <column label="PARAMETER_TEXT" name="Prg" sort-field="Prg" align="center" charlength="40" component="progress" value="40" server-action="data" target-action="QryChkPrg">
                <dependency source-type="launcher" target-type="enable-autorefresh" initial="true" target-action="autorefresh">
                  <dependency-element id="SetAutoload" condition="eq" value="1" view="base" />
                  <dependency-element id="RefreshTime" alias="autorefresh" view="base" />
                </dependency>
              </column>
            </group-header>
            <column label="PARAMETER_TEXT" name="DwnFil" sort-field="Des" align="left" charlength="50" component="text-view" icon="download" unit="5">
              <dependency >
                <dependency-element id="GrdSta" column="DwnFil" attribute="selectedRowValue" event="click" />
                <dependency-action type="server-download" server-action="get-file-maintain" target-action="downloadFileGrid" />
              </dependency>
            </column>
            <column label="PARAMETER_TEXT" name="TxtViw" sort-field="Des" align="right" charlength="10" component="text-view" />
            <column label="PARAMETER_TEXT" name="TxtSty" sort-field="Des" align="right" charlength="10" />
            <context-button id="CtxGrdStaAdd" label="BUTTON_NEW" icon="plus-circle">
              <button-action type="add-row" target="GrdSta" />
              <dependency target-type="show" initial="true">
                <dependency-element id="GrdSta" column="Als" attribute="selectedRowValue" condition="ne" value="awemadora05" />
                <dependency-element id="GrdSta" event="select-row" />
              </dependency>
            </context-button>
            <context-button id="CtxGrdStaDel" label="BUTTON_DELETE" icon="trash">
              <button-action type="delete-row" target="GrdSta" silent="true" />
              <dependency target-type="show" initial="true">
                <dependency-element id="GrdSta" column="Als" attribute="selectedRowValue" condition="eq" value="awemadora05" />
                <dependency-element id="GrdSta" event="select-row" />
              </dependency>
            </context-button>
          </grid>
        </window>
      </tabcontainer>
      <tabcontainer id="MusMat">
        <criteria id="Pru" value="FndPos" component="hidden" />
        <grid id="GrdMus" style="expand grid-bordered" multiselect="true" row-numbers="false" initial-load="query" server-action="data" target-action="QryUniTstId" max="30">
          <column label="PARAMETER_TEXT" name="Als1" sort-field="Als" align="left" charlength="20">
            <dependency target-type="hide-column" initial="true">
              <dependency-element id="GrdMus" attribute="selectedRows" condition="ne" value="3" />
            </dependency>
          </column>
          <column label="PARAMETER_TEXT" name="Des1" sort-field="Des" align="left" charlength="40" />
          <column label="PARAMETER_NAME" name="DesTxt" sort-field="Als" align="left" charlength="40" style="highlight" />
          <column label="COLUMN_NUMERIC" name="MulNum" sort-field="Des" align="right" charlength="20" />
          <dependency target-type="show" initial="true">
            <dependency-element id="GrdSta" attribute="selectedRows" condition="ne" value="1" />
          </dependency>
          <dependency source-type="launcher" target-type="enable-autorefresh" initial="true" target-action="autorefresh">
            <dependency-element id="SetAutoload" condition="eq" value="1" view="base" />
            <dependency-element id="RefreshTime" alias="autorefresh" view="base" />
          </dependency>
        </grid>
      </tabcontainer>
      <tabcontainer id="EdiMat">
        <grid id="GrdEdi" style="expand grid-bordered" editable="true" initial-load="query" server-action="data" target-action="QryEdiTst" max="10" show-totals="true" send-all="true">
          <column label="PARAMETER_TEXT" name="Txt" sort-field="Txt" align="left" component="text" charlength="14" />
          <column label="COLUMN_NUMERIC" name="Num" sort-field="Num" align="right" component="numeric" number-format="{vMin: '-999999', mDec: 2, aSign:' EUR', pSign:'s', aPad:true}"
            value="-123456.1" charlength="16" formatter="number" summary-type="sum">
            <dependency source-type="criteria-value" target-type="input" target-action="CpyNum2">
              <dependency-element id="GrdEdi" column="Num2" attribute="selectedRowValue" alias="CpyNum2" />
            </dependency>
            <dependency initial="true" source-type="formule" target-type="format-number" formule="{vMin: '-999999', mDec: 2, aSign:' $', pSign:'s', aPad:true}">
              <dependency-element id="GrdEdi" column="Txt" condition="ne" value="clean" attribute="currentRowValue" />
            </dependency>
          </column>
          <column label="COLUMN_NUMERIC" name="Num2" sort-field="Num" align="right" component="numeric" number-format="{vMin: '-999999', mDec: 2, aSign:' EUR', pSign:'s', aPad:true}"
            value="0.0" charlength="16" formatter="number" summary-type="sum">
            <dependency source-type="criteria-value" target-type="input" target-action="CpyNum">
              <dependency-element id="GrdEdi" column="Num" attribute="selectedRowValue" alias="CpyNum" />
            </dependency>
            <!--<dependency > <dependency-element id="GrdEdi" column="Num2" attribute="selectedRowValue" value="111"/> <dependency-action type="server" server-action="control-target-message"
              target-action="OpeLst"/> </dependency> -->
          </column>
          <column label="PARAMETER_DATE" name="Dat" sort-field="Dat" align="center" component="date" charlength="16" style="highlight">
            <dependency source-type="criteria-value" target-type="input" target-action="Date" initial="true">
              <dependency-element id="GrdEdi" attribute="selectedRowValue" column="FilDat" alias="Date" />
              <dependency-element id="GrdEdi" column="Dat" attribute="currentRow" condition="eq" id2="GrdEdi" column2="Dat" attribute2="nextRow" />
              <dependency-element id="GrdEdi" event="save-row" />
            </dependency>
            <dependency target-type="show-column" initial="true">
              <dependency-element id="GrdEdi" column="FilDat" attribute="hasDataColumn" />
            </dependency>
          </column>
          <column label="PARAMETER_FILTERED_DATE" name="FilDat" sort-field="FilDat" align="center" component="filtered-calendar" initial-load="query" target-action="FilCalDat" charlength="16">
            <dependency source-type="criteria-value" target-type="input" target-action="Date" initial="true">
              <dependency-element id="GrdEdi" attribute="selectedRowValue" column="Dat" alias="Date" />
              <dependency-element id="GrdEdi" column="FilDat" attribute="currentRow" condition="eq" id2="GrdEdi" column2="FilDat" attribute2="prevRow" />
              <dependency-element id="GrdEdi" event="save-row" />
            </dependency>
          </column>
          <column label="PARAMETER_TIME" name="Tim" sort-field="Tim" align="center" component="time" charlength="14" />
          <group-header name="GrpHea1" label="PARAMETER_TEXT">
            <column label="PARAMETER_SUGGEST" name="Sug" sort-field="Sug" align="left" component="suggest" server-action="data" target-action="QryEdiSug" charlength="20">
              <dependency source-type="reset" target-type="input">
                <dependency-element id="GrdEdi" column="Txt" attribute="selectedRowValue" />
                <dependency-element id="GrdEdi" column="Num" attribute="selectedRowValue" />
              </dependency>
            </column>
            <column label="PARAMETER_SELECT" name="Sel" sort-field="Sel" align="left" component="select" initial-load="enum" target-action="Es1Es0" optional="true" charlength="20">
              <dependency source-type="criteria-value" target-type="label" target-action="Date">
                <dependency-element id="GrdEdi" column="Dat" attribute="selectedRowValue" alias="Date" />
              </dependency>
            </column>
            <column label="PARAMETER_TEXTAREA" name="Tar" sort-field="Tar" align="left" component="textarea" charlength="40" />
            <column label="PARAMETER_UPLOADER" name="Upl" sort-field="Upl" align="left" component="uploader" charlength="30" />
          </group-header>
          <column label="PARAMETER_COLOR" name="Col" sort-field="Col" align="center" component="color" charlength="20" />
          <column label="PARAMETER_CHECKBOX" name="ChkBox" sort-field="ChkBox" align="center" component="checkbox" type="boolean" charlength="12" />
          <column label="PARAMETER_DIALOG" name="Dialog" sort-field="Dialog" align="center" icon="plus-circle" component="dialog" dialog="dialogo-modal" server-action="data-silent"
            target-action="ModalDialog" charlength="12">
            <dependency target-type="show" initial="true">
              <dependency-element id="GrdEdi" attribute="selectedRow" condition="eq" id2="GrdEdi" attribute2="currentRow" />
            </dependency>
          </column>
          <button label="BUTTON_NEW" icon="plus-circle" id="ButGrdEdiAdd">
            <button-action type="add-row" target="GrdEdi" silent="true" />
          </button>
          <button label="BUTTON_DELETE" icon="trash" id="ButGrdEdiDel">
            <button-action type="delete-row" target="GrdEdi" silent="true" />
            <dependency target-type="enable" initial="true">
              <dependency-element id="GrdEdi" attribute="selectedRows" condition="eq" value="1" />
            </dependency>
          </button>
          <context-button id="CtxGrdEdiAdd" label="BUTTON_NEW" icon="plus-circle">
            <button-action type="add-row" target="GrdEdi" silent="true" />
            <dependency target-type="show" initial="true">
              <dependency-element id="GrdEdi" attribute="selectedRows" condition="eq" value="0" />
            </dependency>
          </context-button>
          <context-button id="CtxGrdEdiAddSel" label="BUTTON_NEW" icon="plus-circle">
            <context-button label="BUTTON_NEW_ROW_UP" id="CtxGrdEdiAddUpp" icon="level-up">
              <button-action type="add-row-up" target="GrdEdi" silent="true" />
            </context-button>
            <context-button label="BUTTON_NEW_ROW_DOWN" id="CtxGrdEdiAddDow" icon="level-down">
              <button-action type="add-row-down" target="GrdEdi" silent="true" />
            </context-button>
            <dependency target-type="show" initial="true">
              <dependency-element id="GrdEdi" attribute="selectedRows" condition="eq" value="1" />
            </dependency>
          </context-button>
          <context-button id="CtxGrdEdiDel" label="BUTTON_DELETE" icon="trash">
            <button-action type="delete-row" target="GrdEdi" silent="true" />
            <dependency target-type="enable" initial="true">
              <dependency-element id="GrdEdi" attribute="selectedRows" condition="eq" value="1" />
            </dependency>
          </context-button>
        </grid>
      </tabcontainer>
      <tabcontainer id="EdiMusMat">
        <window style="expand" expandible="vertical">
          <grid id="GrdEdiMus" style="expand" multiselect="true" row-numbers="false" editable="true" initial-load="query" server-action="data" target-action="QryEdiTst" max="10">
            <column label="PARAMETER_TEXT" name="Txt1" sort-field="Txt" align="left" component="text" charlength="14" />
            <column label="COLUMN_NUMERIC" name="Num1" sort-field="Num" align="right" component="numeric" number-format="{vMin: '-999999', mDec: 2, aSign:' EUR', pSign:'s', aPad:true}"
              value="-123456.1" charlength="30" />
            <column label="PARAMETER_DATE" name="Dat1" sort-field="Dat" align="center" component="date" charlength="16">
              <dependency source-type="criteria-value" target-type="input" target-action="Date" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRowValue" column="FilDat1" alias="Date" />
                <dependency-element id="GrdEdiMus" column="Dat1" attribute="currentRow" condition="eq" id2="GrdEdiMus" column2="Dat1" attribute2="nextRow" />
                <dependency-element id="GrdEdiMus" event="save-row" />
              </dependency>
              <dependency target-type="hide-column" initial="true">
                <dependency-element id="GrdEdiMus" column="Dat1" attribute="emptyDataColumn" />
              </dependency>
            </column>
            <column label="PARAMETER_FILTERED_DATE" name="FilDat1" sort-field="FilDat" align="center" component="filtered-calendar" initial-load="query" target-action="FilCalDat" charlength="16">
              <dependency source-type="criteria-value" target-type="input" target-action="Date" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRowValue" column="Dat1" alias="Date" />
                <dependency-element id="GrdEdiMus" column="FilDat1" attribute="currentRow" condition="eq" id2="GrdEdiMus" column2="FilDat1" attribute2="prevRow" />
                <dependency-element id="GrdEdiMus" event="save-row" />
              </dependency>
            </column>
            <column label="PARAMETER_TIME" name="Tim1" sort-field="Tim" align="center" component="time" charlength="14" />
            <column label="PARAMETER_SUGGEST" name="Sug1" sort-field="Sug" align="left" component="suggest" server-action="data" target-action="QryEdiSug" charlength="20" />
            <column label="PARAMETER_SELECT" name="Sel1" sort-field="Sel" align="left" component="select" initial-load="enum" target-action="Es1Es0" optional="true" charlength="20">
              <dependency source-type="criteria-value" target-type="label" target-action="Date">
                <dependency-element id="GrdEdiMus" column="Dat1" attribute="selectedRowValue" alias="Date" />
              </dependency>
            </column>
            <column label="PARAMETER_TEXTAREA" name="Tar1" sort-field="Tar" align="left" component="textarea" charlength="40" />
            <column label="PARAMETER_UPLOADER" name="Upl1" sort-field="Upl" align="left" component="uploader" charlength="30" />
            <column label="PARAMETER_COLOR" name="Col1" sort-field="Col" align="center" component="color" charlength="20" />
            <column label="PARAMETER_CHECKBOX" name="ChkBox1" sort-field="ChkBox" align="center" component="checkbox" charlength="12" />
            <column label="PARAMETER_DIALOG" name="Dialog2" sort-field="Dialog2" align="center" icon="plus-circle" component="dialog" dialog="dialogo-modal" server-action="data-silent"
              target-action="ModalDialog" charlength="12">
              <dependency target-type="show" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRows" condition="eq" value="1" />
                <dependency-element id="GrdEdiMus" attribute="selectedRow" condition="eq" id2="GrdEdiMus" attribute2="currentRow" />
              </dependency>
            </column>
            <button label="BUTTON_NEW" icon="plus-circle" id="ButGrdEdiMusAdd">
              <button-action type="add-row" target="GrdEdiMus" silent="true" />
            </button>
            <button label="BUTTON_DELETE" icon="trash" id="ButGrdEdiMusDel">
              <button-action type="delete-row" target="GrdEdiMus" silent="true" />
              <dependency target-type="enable" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRows" condition="eq" value="1" />
              </dependency>
            </button>
            <context-button id="CtxGrdEdiMusAdd" label="BUTTON_NEW" icon="plus-circle">
              <button-action type="add-row" target="GrdEdiMus" silent="true" />
              <dependency target-type="show" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRows" condition="eq" value="0" />
              </dependency>
            </context-button>
            <context-button id="CtxGrdEdiMusAddSel" label="BUTTON_NEW" icon="plus-circle">
              <context-button label="BUTTON_NEW_ROW_UP" id="CtxGrdEdiMusAddUpp" icon="level-up">
                <button-action type="add-row-up" target="GrdEdiMus" silent="true" />
              </context-button>
              <context-button label="BUTTON_NEW_ROW_DOWN" id="CtxGrdEdiMusAddDow" icon="level-down">
                <button-action type="add-row-down" target="GrdEdiMus" silent="true" />
              </context-button>
              <dependency target-type="show" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRows" condition="eq" value="1" />
              </dependency>
            </context-button>
            <context-button id="CtxGrdEdiMusDel" label="BUTTON_DELETE" icon="trash">
              <button-action type="delete-row" target="GrdEdiMus" silent="true" />
              <dependency target-type="enable" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRows" condition="eq" value="1" />
              </dependency>
            </context-button>
            <context-button id="CtxGrdEdiMusCpy" label="BUTTON_COPY" icon="clipboard">
              <button-action type="copy-selected-rows-clipboard" target="GrdEdiMus" silent="true" />
              <dependency target-type="enable" initial="true">
                <dependency-element id="GrdEdiMus" attribute="selectedRows" condition="ge" value="1" />
              </dependency>
            </context-button>
          </grid>
        </window>
      </tabcontainer>
      <tabcontainer id="MuoMat">
        <window style="expand" expandible="vertical">
          <grid id="GrdMuo" style="expand" editable="true" send-operations="true" initial-load="query" server-action="data" target-action="QryUniTst" max="30">
